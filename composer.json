{
    "name": "roadrunner-php/symfony-lock-driver",
    "type": "library",
    "description": "RoadRunner: symfony/lock bridge",
    "keywords": [
        "roadrunner-php",
        "symfony",
        "roadrunner",
        "spiral",
        "lock"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Buchnev (butschster)",
            "email": "pavel.buchnev@spiralscout.com"
        },
        {
            "name": "Alexander Strizhak",
            "email": "gam6itko@gmail.com"
        }
    ],
    "homepage": "https://roadrunner.dev",
    "support": {
        "docs": "https://docs.roadrunner.dev",
        "issues": "https://github.com/roadrunner-server/roadrunner/issues",
        "forum": "https://forum.roadrunner.dev/",
        "chat": "https://discord.gg/V6EK4he"
    },
    "require": {
        "php": ">=8.1",
        "roadrunner-php/lock": "^1.0",
        "symfony/lock": "^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.9"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\RoadRunner\\Symfony\\Lock\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\RoadRunner\\Symfony\\Lock\\Tests\\": "tests"
        }
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/roadrunner-server"
        }
    ],
    "scripts": {
        "analyze": "psalm"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
